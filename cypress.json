{
  "baseUrl": "http://localhost:4200",
  "ignoreTestFiles": [
    "**/examples/*",
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "viewportHeight": 1080,
  "viewportWidth": 1920,
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "configFile": "reporter-config.json"
  },
  "retries": {
    "runMode": 2,
    "openMode": 1
  },
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.01
      }
    }
  }
}
